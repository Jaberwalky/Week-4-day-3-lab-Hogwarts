<table>
  <tr>
    <th>Name</th>
    <th>House</th>
    <th>Age</th>
    <th>Logo</th>

  </tr>
  <% @students.each do |student| %>
  <tr>
    <td> <%= student.first_name + " " + student.second_name %> </td>
    <td> <%= student.house_name() %> </td>
    <td> <%= student.age %> </td>
    <td> <%= student.house_logo() %> </td>
    

  </tr>
  <% end %>

